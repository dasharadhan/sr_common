<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:macro name="shadowhand" params="muscletrans muscle bio bt_sp eli reflect prefix">
    ${xacro.warning("The file three_fingers_hand_RF.urdf.xacro is deprecated", print_location=False)}
    ${xacro.warning("Use hand.urdf.xacro instead with fingers='th ff rf'")}
    <!-- forward to generic macro -->
    <xacro:include filename="$(find sr_description)/hand/xacro/hand.urdf.xacro" />
    <xacro:sr_hand fingers="th ff rf" reflect="${reflect}" prefix="${prefix}" />
  </xacro:macro>
</robot>
